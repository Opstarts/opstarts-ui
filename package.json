{
  "name": "opstarts-ui",
  "version": "0.0.2-4",
  "description": "Opstarts React UI components",
  "main": "index.jsx",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/*test.js",
    "test:watch": "mocha test/*test.js --compilers js:babel-core/register --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opstarts/opstarts-ui.git"
  },
  "keywords": [
    "react",
    "opstarts"
  ],
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "author": "Billy Vong <billyvg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Opstarts/opstarts-ui/issues"
  },
  "homepage": "https://github.com/Opstarts/opstarts-ui#readme",
  "dependencies": {
    "classnames": "^2.2.0",
    "react": "^0.14.2",
    "react-addons-transition-group": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.3",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "expect": "^1.12.2",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.2"
  }
}
